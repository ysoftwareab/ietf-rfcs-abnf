M=mandatory, O=optional, I=ignored


enabled=<boolean>
owner=<owner-name>

api-path = "/" api-identifier
                       0*("/" (api-identifier | key-value ))

api-select = api-identifier
                       0*("/" (api-identifier | key-value ))

api-identifier = [module-name ":"] identifier

module-name = identifier

key-value = string

identifier  = (ALPHA / "_")
                     *(ALPHA / DIGIT / "_" / "-" / ".")

string = <an unquoted string>

