NSS = (family ":" number) / ("mtg-" number "-" wgbofname)
                      ( "id:" name)
family = "rfc" / "std" / "fyi" / "bcp"
number = 1*DIGIT
wgbofname = 1*LETDIGIT
name =  id draft designation with "draft-" and trailing
                        type removed (e.g. ietf-urn-ietf-06)
LETDIGIT = DIGIT / %x41-%x5a / %x61-%x7a
DIGIT = %x30-%x39

