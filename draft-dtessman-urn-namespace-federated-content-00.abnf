NSS         = ProviderId ":" DateId ":" ResourceId
ProviderId  = 1*(label ".") toplabel
DateId      = ("00" 6(DIGIT)) | (cc yy mm dd)             
ResourceId  = 1*(alphadig / other / ("%" HEXDIG HEXDIG))
label       = alphadig / alphadig *[ alphadig / "-" ] alphadig
toplabel    = ALPHA / ALPHA *[ alphadig / "-" ] alphadig
cc          = ("0" posdig) / (posdig DIGIT)
yy          = 2(DIGIT)
mm          = ("0" posdig) / ("1" ("0" "1" "2"))
dd          = ("0" posdig) / (( "1" / "2") DIGIT) / "30" / "31"
other       = "(" / ")" / "+" / "," / "-" / "." / ":" / "=" /
                    "@" / ";" / "$" / "_" / "!" / "*" / "'"
alphadig    = ALPHA / DIGIT
posdig      = %x31-39

