sha1uri   = "sha1:" ( 40HEXDIG / 40*relaxed ) [ length ]

sha256uri = "sha256:" ( 64HEXDIG / 64*relaxed ) [ length ]

relaxed   = HEXDIG / ":" / "." / "~" / "-" / "_" /
                     "%20" / "%0D" / "%0A" / "%09"

length    = ";" wholenum [ bits ]

wholenum  = "0" / ( %x31-39 *DIGIT )

bits      = "b" ( %x31-37 )   ; 1 - 7

