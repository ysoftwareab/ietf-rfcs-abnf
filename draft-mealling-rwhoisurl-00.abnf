RWHOISURL = "rwhois://" [ SERVER ] "/" [ QUERY ]

SERVER    = 1 *DNSCHAR *["." 1*DNSCHAR] [ ":" 1..65535 ]

QUERY     = [ CLASS ] "?" TERMS

CLASS     = 1*ALPHA

TERMS     = a list of query terms as defined in RFC 1714

DNSCHAR   = ALPHA/DIGIT/DASH

ALPHA     = "a".."z"/"A".."Z"

DIGIT     = 0..9

DASH      = "-"

