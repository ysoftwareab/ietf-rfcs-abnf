cpc           = cpc-tag "=" cpc-value
cpc-tag       = "cpc"
cpc-value     = "ordinary" / "prison"   / "police"  / "test"
                      "operator" / "payphone" / "unknown" /
                      "hospital" / "cellular" / "cellular-roaming" /
                       genvalue

genvalue      = 1*(alphanum / "-" / "." )

