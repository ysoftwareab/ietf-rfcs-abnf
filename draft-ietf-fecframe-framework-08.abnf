scheme-specific-info  = [ element *( ',' element ) ]
element               = name ':' value
name                  = token
token                 = 1*<any CHAR except CTLs or separators>
value                 = *<any CHAR except CTLs or separators>
separators            = "(" | ")" | "<" | ">" | "@"
                                | "," | ";" | ":" | "\" | <">
                                | "/" | "[" | "]" | "?" | "="
                                | "{" | "}" | SP | HT

