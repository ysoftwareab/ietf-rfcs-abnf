start =
       element tld {
         attribute levels { xsd:integer }?,
         attribute name { xsd:NCName }?,
         (domain | registry)*
       }
registry =
     element registry {
       attribute levels { xsd:integer }?,
       attribute name { xsd:NCName },
       (domain | registry)*
     }
domain =
     element domain {
       attribute name { xsd:NCName }
     }

