csv-fragment =  headersel / wheresel / colsel / rowsel
headersel = "head"
rowsel   = "row:" rowspec
colsel   = "col:" colspec
wheresel = "where:" kvpairs
kvpairs = 1*( col "=" val 0*1(",") )
col = 1*TEXTDATA
val = 1*TEXTDATA
colspec = column
rowspec = "*" / rownum
column = 1*TEXTDATA / 1*DIGIT
rownum = 1*DIGIT
TEXTDATA =  %x23-2B / %x2D-3C / %x3E-7E
DIGIT =  %x30-39


