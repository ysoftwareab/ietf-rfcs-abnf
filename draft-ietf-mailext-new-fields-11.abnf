                                auto-submitted

auto-submitted           = ( "no" / "auto-generated" /
                                "auto-replied" /
                                private-extension /
                                future-extension )
                                optional-parameter-list

optional-parameter-list   = *( ";" parameter> )

private-extension         = x-token

Future-extension          = token

