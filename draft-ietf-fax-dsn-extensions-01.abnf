extension-field = call-duration / transmitted-pages

call-duration     = "Fax-Call-Duration" ":" elapsed-time
transmitted-pages = "Fax-Transmitted-Pages" ":" xmit-pages

elapsed-time = hour ":" minute ":" second [ ":" hundred ]
      
hour    = 2DIGIT
minute  = 2DIGIT
second  = 2DIGIT
hundred = 2DIGIT
      
xmit-pages = 1*DIGIT
      
