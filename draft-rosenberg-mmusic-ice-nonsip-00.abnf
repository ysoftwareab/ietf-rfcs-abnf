NICE-obj    =     nice-ufrag CRLF
                     nice-pwd CRLF
                     nice-proto CRLF
                     1*(nice-cand CRLF)
                     *(nice-opts CRLF)
                     *(nice-ext CRLF)
nice-ufrag  =     ice-pwd-att
nice-pwd    =     ice-ufrag-att
nice-cand   =     candidate-attribute
nice-opts   =     ice-options
nice-proto  =     "nextproto:" token
nice-ext    =     ext-name ":" ext-value
ext-name    =     token
ext-value   =     byte-string

