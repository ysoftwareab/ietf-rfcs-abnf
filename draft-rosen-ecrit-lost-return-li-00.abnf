start =
        responseLocation


findServiceResponse =
          extensionPoint+,
          attribute profile { xsd:NMTOKEN }?
responseLocation =
       element location {
         attribute id { xsd:token },
         locationInformation
       }+
notLostRP = element * - (ns1:* | ns1:*) { anyElement }

anyElement =
          (element * { anyElement }
           | attribute * { text }
           | text)*

extensionPoint = notLostRP*
content="text/html;charset=iso-8859-1"/>
