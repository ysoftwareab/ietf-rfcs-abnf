fields =/ downgraded
downgraded  = "Downgraded:" [CFWS] field-name ":" dvalue CRLF
field-name = 1*ftext
ftext      = %d33-57 / %d59-126 ; printable ASCII except ":"
dvalue     = 1*any-ascii
any-ascii  = %d32-126

fields =/ edowngraded
edowngraded = "Envelope-Downgraded:" [CFWS] field-name ":" value CRLF
field-name = "From" / "To"
value = CFWS "<" uPath ">" CFWS "<" Mailbox ">" CFWS

