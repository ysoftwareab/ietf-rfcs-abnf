depend-attribute =
             "a=depend:" dependent-fmt SP dependency-tag
                *(";" SP dependent-fmt SP dependency-tag) CRLF

dependency-tag   =
             dependency-type *1( SP identification-tag ":"
             fmt-dependency *("," fmt-dependency ))

dependency-type  = "lay"
                      / "mdc"
                      / token

dependent-fmt = fmt
fmt-dependency = fmt

