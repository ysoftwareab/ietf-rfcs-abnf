EKT_Plaintext = SRTP_Master_Key || SSRC || ROC || ISN

EKT_Ciphertext = EKT_Encrypt(EKT_Key, EKT_Plaintext)

Full_EKT_Field = EKT_Ciphertext || SPI || '1'

Short_EKT_Field = Reserved || '0'

