href="http://example.org/reilly/pic" />
href="http://example.org/reilly/feed" />
title="Books"
href="http://example.org/reilly/books" />
appService =
      element app:service {
         appCommonAttributes,
         ( appWorkspace*
           & anyElement* )
      }


appWorkspace =
      element app:workspace {
         appCommonAttributes,
         attribute title { text },
         ( appCollection*
           & anyElement* )
      }
appCollection =
      element app:collection {
         appCommonAttributes,
         attribute title { text },
         attribute href { text },
         attribute contents { text },
         anyElement*
      }


Title     = "Title" ":" [text]
