appCommonAttributes =
         attribute xml:base { atomUri }?,
         attribute xml:lang { atomLanguageTag }?,
        undefinedAttribute*


appCollection =
      element app:collection {
         appCommonAttributes,
         ( appMemberType+
           appSearchTemplate
           & anyElement* )
      }


appMember =
      element app:member-type {
            appCommonAttributes,
            appTypeValue
      }


