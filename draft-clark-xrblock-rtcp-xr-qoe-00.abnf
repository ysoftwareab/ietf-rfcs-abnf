rtcp-xr-attrib = "a=" "rtcp-xr" ":" [xr-format *(SP xr-format)]
           CRLF (defined in RFC3611)

xr-format = xr-format / 
                qoe-metrics

qoe-metrics   = "qoe-metrics" [EQUAL word]
DIGIT          = %x30-39
format-ext     = non-ws-string
non-ws-string  = 1*(%x21-FF)
CRLF           = %d13.10


