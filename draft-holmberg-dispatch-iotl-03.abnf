uri-parameter =/ iotl-param
iotl-param    = iotl-tag "=" iotl-value ["." iotl-value]
iotl-tag      = "iotl"
iotl-value    = "homeA-homeB" / "homeB-visitedB" / "visitedA-homeA"
                   / "homeA-visitedA" /" visitedA-homeB" / other-iotl
other-iotl    = 1*iotl-char
iotl-char     = alphanum / "-"
