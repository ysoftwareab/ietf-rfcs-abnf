simpleExtensionElement =
      element * - app:* {
         text
      }

structuredExtensionElement =
      element * - app:* {
         (attribute * { text }+,
            (text|anyElement)*)
       | (attribute * { text }*,
          (text?, anyElement+, (text|anyElement)*))
      }

extensionElement =
      simpleExtensionElement | structuredExtensionElement
undefinedContent = (text|anyForeignElement)*

anyElement =
