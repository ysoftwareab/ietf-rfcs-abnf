header-request =  "Header-Request" ":" h-rq h-param
h-rq           =  h-element *( "," h-element )
h-element      =  token
h-param        =  *( ";" h-param-av )
h-param-av     =  "Domain" "=" value ; *1*
                           /  "Path" "=" value   ; *1*
                           /  "Secure"           ; *1*

