mime-charset = 1*<Any CHAR except SPACE, CTLs, and cspecials>

cspecials    = "(" / ")" / "<" / ">" / "@" / "," / ";" / ":" / "
                  <"> / "/" / "[" / "]" / "?" / "." / "=" / "*"

CHAR         =  <any ASCII character>        ; (  0-177,  0.-127.)
SPACE        =  <ASCII SP, space>            ; (     40,      32.)
CTL          =  <any ASCII control           ; (  0- 37,  0.- 31.)
                    character and DEL>          ; (    177,     127.)

