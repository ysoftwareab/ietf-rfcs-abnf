Feature       = "EDIINT-Features:" [WSP] Feature-Name *([WSP] ","
                   [WSP] Feature-Name)

Feature-Name  = 1*Feature-Token

Feature-Token = %d48-57 / ; 0-9
                   %d65-90 / ; A-Z
                   %d97-122 / ; a-z
                   "-" ; hyphen is allowed
                   ; blank space " " is not allowed
