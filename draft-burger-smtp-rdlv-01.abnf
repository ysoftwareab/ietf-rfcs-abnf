seconds     = 1*DIGIT
                 ; 31-bit positive integer
                 ; (1 <= n < 2,147,483,648)

rdlv-param  = "mintimer" "=" seconds
                 ; parameter to RDLV EHLO keyword.
                 ; matches "ehlo-keyword"

rdlv-cmd    = "RDLV" SP "maxtimer" "=" seconds CRLF

cont-cmd    = "CONT"


