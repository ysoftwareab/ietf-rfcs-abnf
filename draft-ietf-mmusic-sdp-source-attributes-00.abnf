ssrc-attr = "ssrc:" ssrc-id SP attribute
ssrc-id = integer ; 0 - 2**32 - 1

ssrc-group-attr = "ssrc-group:" semantics *(SP ssrc-id)
cname-attr = "cname:" cname
cname = byte-string
previous-ssrc-attr = "previous-ssrc:" ssrc-id *(SP ssrc-id)

