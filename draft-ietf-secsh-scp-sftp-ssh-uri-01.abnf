ssh_URI = "ssh://" [ userinfo "@" ] host [ ":" port ] 
         [;conn-parameter=value] 
    
scp_URI = "scp://" [ userinfo "@" ] host [ ":" port ]  
         [ ; parameter = value ] [ abs_path ] 
    
sftp_URI = "sftp://" [ userinfo "@" ] host [ ":" port ] 
         [;conn-parameter=value] [ abs_path ] [;sftp-parameter=value] 
    
