from            =   "From:" address-list CRLF
resent-from     =   "Resent-From:" address-list CRLF
sender          =   "Sender:" address CRLF
resent-sender   =   "Resent-Sender:" address CRLF


fields                   =/ known-downgraded-headers ":"
                               unstructured CRLF

known-downgraded-headers =  "Downgraded-" original-headers

original-headers         =  "Message-Id" / "Resent-Message-Id" /
                               "In-Reply-To:" / "References:" /
                               "Original-Recipient" / "Final-Recipient"


