networkdomainname = maskedoctet "." *( decimaloctet / maskedoctet ".")
                        ".in-addr.arpa."

maskedoctet = decimaloctet "-" mask

mask = (*1( "1" / "2" ) DIGIT ) / "3" ("1" / "2" )

decimaloctet = ( *1("1") DIGIT DIGIT ) /
                   ( "2" ( "1" / "2" / "3" / "4" ) DIGIT ) /
                   ( "2" "5" ( "1" / "2" /"3" / "4" / "5" ) )

IPv4CIDR = decimaloctet "." decimaloctet "." decimaloctet "."
