namestring    = assigned-name
                      [ p-component ]
                      [ q-component ]
                      [ f-component ]
assigned-name = "urn" ":" NID ":" NSS
                    ; the URI scheme ("urn") is case insensitive
NID           = (alphanum) 0*30(ldh) (alphanum)
ldh           = alphanum / "-"
NSS           = 1*(pchar)
p-component   = "/" path-absolute
q-component   = "?" query
f-component   = "#" fragment

