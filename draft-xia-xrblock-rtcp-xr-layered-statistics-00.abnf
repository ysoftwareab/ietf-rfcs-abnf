rtcp-xr-attrib =  "a=rtcp-xr:"
                      [xr-format *(SP xr-format)] CRLF
xr-format = layered-stream-stat-metrics
layered-stream-stat-metrics = "layered-stream-stat-metrics"
                                  ["=" stat-flag *("," stat-flag)]
stat-flag = "base layer packet"
                               / "enhancment layer packet"
