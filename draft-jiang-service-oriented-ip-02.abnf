sat-packet = [sat, flags, traffic-class, session-id, hop-limit,
                 source, service-data, ?payload]

sat = 0..255
flags = bytes .size 1
traffic-class = 0..255
session-id = 0..4294967295 ;up to 32 bits
hop-limit = 0..255
client = ipv6-address
service-data = any
payload = any

ipv6-address = bytes .size 16

