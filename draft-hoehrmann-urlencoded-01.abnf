data-set  = empty-set / pairs
pairs     = pair *(seperator pair)
pair      = name [ "=" value ]
name      = *(namechar / escape / percent / plus)
value     = *(valuechar / escape / percent / plus)
namechar  = <any octet except ";", "&", "+", "%", "=">
valuechar = <any octet except ";", "&", "+", "%">
escape    = "%" 2hexdig
separator = ";" / "&"
percent   = "%"
plus      = "+"
empty-set = ""

