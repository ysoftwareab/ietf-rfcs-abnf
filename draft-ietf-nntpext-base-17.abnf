wildmat = wildmat-pattern *("," ["!"] wildmat-pattern)

wildmat-pattern = 1*wildmat-item

wildmat-item = wildmat-exact / wildmat-wild

wildmat-exact = %x21-29 / %x2B / %x2D-3E / %x40-5A / %x5E-7E /
         UTF-8-non-ascii ; exclude * , ? [ \ ]

wildmat-wild = "*" / "?"

