message-length =    1*DIGIT 
    
request-id    =    1*DIGIT 
  
method-name    =    generic-method      ; Section 6 
                     |    synthesizer-method 
                     |    recorder-method 
                     |    recognizer-method 
                     |    verifier-method 
     
