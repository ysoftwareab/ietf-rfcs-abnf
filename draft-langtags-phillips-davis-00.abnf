language_tag	= lang ["-" script] ["-" region] *("-" variant) 
  [extensions] 

	=/ "x" *("-" alphanum)

	=/ grandfathered_registrations

lang	= shortest_alpha_ISO_639_code

	=/ registered_lang 

script	= ISO_15924_code

region	= shortest_alpha_ISO_3166_code

	=/ registered_region

variant	= year

	=/ registered_variant
registered_lang	= 5*16 alphanum

registered_variant	= 5*16 alphanum

extensions	= "-x" 1* ("-" key "=" value)

value	= * alphanum

alphanum	= (ALPHA / DIGIT)

year	= * DIGIT

language-range = language-tag / "*"

de-CH-x-collation=phonebook
az-Arab-x-SIL=AZE-dialect=derbend

