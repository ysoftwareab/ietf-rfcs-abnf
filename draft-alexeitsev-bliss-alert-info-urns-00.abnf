alert-URN       = "URN:alert:" indication
indication      = top-level *("." sub-indication)
top-level       = let-dig [ *25let-dig-hyp let-dig ]
sub-indication  = let-dig [ *let-dig-hyp let-dig ]
let-dig-hyp     = let-dig / "-"
let-dig         = ALPHA / DIGIT
ALPHA           = %x41-5A / %x61-7A   ; A-Z / a-z
DIGIT           = %x30-39 ; 0-9


