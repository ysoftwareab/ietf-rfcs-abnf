text-fragment =  1*text-scheme
text-scheme   =  ( char-scheme / line-scheme / regex-scheme )
char-scheme   =  "char(" ( count / range ) ")"
line-scheme   =  "line(" ( count / range ) ")"
match-scheme  =  "match(" regex ")"
count         =  1*DIGIT
range         =  count "," [ count ]
regex         =  StringWithBalancedParens

