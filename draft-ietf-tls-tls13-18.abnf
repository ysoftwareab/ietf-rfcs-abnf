value = (byte[0] << 8*(n-1)) | (byte[1] << 8*(n-2)) |
              ... | byte[n-1];

finished_key =
       HKDF-Expand-Label(BaseKey, "finished", "", Hash.length)

verify_data =
          HMAC(finished_key, Hash(
                                  Handshake Context +
                                  Certificate* +
                                  CertificateVerify*
                             )
          )

AEADEncrypted =
          AEAD-Encrypt(write_key, nonce, plaintext of fragment)

