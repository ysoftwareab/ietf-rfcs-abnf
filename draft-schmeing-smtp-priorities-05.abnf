namespace-list = namespace * (COMMA namespace)
namespace      = token-nodot
token-nodot    = 1*(alphanum / "-" / "!" / "%" / "*" /
                         " " / "+" / "`" / "'" / "~")


priority-arg = "PRIORITY" EQUAL value
value        = namespace "." priority
priority     = token-nodot


PRIORITY=Example.1

PRIORITY=AnotherExample.Flash

