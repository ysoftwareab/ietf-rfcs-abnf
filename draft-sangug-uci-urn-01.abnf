UCI = prefix "-" instance *1(":" qualifier)
      
prefix = 1*(alphaDigit) *1(":" 1*(alphaDigit)) 
               *1("+" 1*(alphaDigit))
instance = 1*(trans / "%" HEXDIG HEXDIG)
qualifier = head 1*(alphaDigit) *2("-" head 1*(alphaDigit))     
trans = alphaDigit / other
alphaDigit = ALPHA / DIGIT
head = "C" / "R" / "F"             
other = "(" / ")" / "+" / "," / "-" / "." / "=" / "@" /
              ";" / "$" / "_" / "!" / "*" / "'"       
reserved = "%" / "/" / "?" / "#"

