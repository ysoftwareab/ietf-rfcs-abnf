mime-charset = 1*mime-charset-chars
mime-charset-chars = ALPHA / DIGIT /
                       "!" / "#" / "$" / "%" / "&" /
                       "'" / "+" / "-" / "^" / "_" /
                       "`" / "{" / "}" / "~"
ALPHA        = "A".."Z"    ; Case insensitive ASCII Letter
DIGIT        = "0".."9"    ; Numeric digit

