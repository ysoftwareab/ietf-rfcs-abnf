reserved  =  "~" / "$"

unreserved  = alpha / DIGIT / "-" / "."

alpha    =  %x61-7A   ; a-z

UTID  =  [ id ] "~" [ catalog ] "$" dns

id         =  1 * 92 graphic

catalog    =  label 0 * 30 dot-label 0 * 1 label

dns        =  label 0 * 60 dot-label "." 2 * 7 alpha

label      =  alpha / DIGIT

dot-label  =  alpha / DIGIT / "-" / "."

alpha      =  %x61-7A   ; a-z

graphic    =  %x21-7E / Graphic Unicode character

id       = $1

catalog  = $2

dns      = $3

