ext-decl        = <"> URI <"> [ ext-params ]
ext-params      = ";" namespace *( ext-extension )

namespace       = ";" "ns" "=" prefix
prefix          = 1*DIGIT "-"
ext-extension   = ";" token [ "=" ( token | quoted-string ) ]

mandatory       = "Man" ":" 1#ext-decl
optional        = "Opt" ":" 1#ext-decl

c-mandatory     = "C-Man" ":" 1#ext-decl
c-optional      = "C-Opt" ":" 1#ext-decl

