a=gpmd:<format><generic parameter list>   
        
m=audio 3456 RTP/AVP 0 15     
a=gpmd:0 vbd=yes 
 
m=application 3456 udp wb 
a=gpmd:wb someParameter=someValue  
 
attribute-fields =    *("a=" attribute CRLF)   
attribute =           (att-field ":" att-value) | att-field 
       
 
gpmd-attribute-field = "a=" gpmd-attribute CRLF 
gpmd-attribute = "gpmd" ":" gpmd-format gpmd-value 
gpmd-format = token 
                    ; for audio media, this is typically  an RTP payload type 
gpmd-value = gpmd-subparameter *(";" gpmd-subparameter) 
gpmd-subparameter = gpmd-subparameter-name "=" gpmd-subparameter-value 
gpmd-subparameter-name = 1*(alpha-numeric)  
gpmd-subparameter-value = token 
token = 1*(token-char) 
alpha-numeric = ALPHA / DIGIT 
                    ; external reference: 
                    ; ALPHA and DIGIT from RFC 2234 [4] 
token-char = %x21/%x23-27/%x2A-2B/%x2D-2E/%x30-39/%x41-5A/%x5E-7E 
                   General-Purpose Media Descriptor      
                    ; definition from RFC 2045 - 
                    ; "any (US-ASCII) CHAR except SPACE, CTLs, 
                    ; or tspecials". 
                    ; the tspecials are ()<>@,;: 
 
 
m=audio 3456 RTP/AVP 0 15 98  
a=rtpmap:98 PCMU/8000   
a=gpmd:98 vbd=yes 
 
m=audio 3456 RTP/AVP 0 15 98  
a=gpmd:0 vbd=yes  
a=rtpmap:98 PCMU/8000   
 
