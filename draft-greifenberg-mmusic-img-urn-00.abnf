NSS            = ProviderId ":" DateId ":" IMGResourceId
                        [":" FragmentId]
ProviderId     = 1*(label ".") toplabel
DateId         = CCYY MM DD
IMGResourceId  = 1*(alphanum / symbol / ("%" hex hex))
FragmentId     = 1*(alphanum / symbol / ":" / ("%" hex hex))
label          = alphanum / alphanum *[ alphanum / "-" ] alphanum
toplabel       = ALPHA / ALPHA *[ alphanum / "-" ] alphanum
CCYY           = 4(DIGIT)
MM             = ( 0 posdig ) / ( "1" ( "0" "1" "2" ) )
DD             = ( 0 posdig ) / ( ( "1" / "2" ) DIGIT ) / "30" /
                        "31"
alphanum       = ALPHA / DIGIT
hex            = DIGIT / "A" / "B" / "C" / "D" / "E" / "F" /
                        "a" / "b" /  "c" / "d" / "e" / "f"
symbol         = "(" / ")" / "+" / "," / "-" / "." / "=" /
                        "@" / ";" / "$" / "_" / "!" / "*" / "'"

