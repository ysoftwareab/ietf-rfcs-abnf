rtcp-xr-attrib =  "a=rtcp-xr:"
                     [xr-format *(SP xr-format)] CRLF
xr-format = multimedia-quality-metrics
multimedia-quality-metrics = "multimedia-quality-metrics"
                                  ["=" stat-flag *("," stat-flag)]
stat-flag = "Interval Metrics"
                            /"Cumulative metrics"

