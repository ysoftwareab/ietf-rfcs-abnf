middle-septet = %d128-255
end-septet    = %d000-127
cardinal      = *middle-septet end-septet

x00           = %d000 / %d128 x00
x01           = %d001 / %d129 x00
x02           = %d002 / %d130 x00
x03           = %d003 / %d131 x00
x04           = %d004 / %d132 x00
x05           = %d005 / %d133 x00
x06           = %d006 / %d134 x00
x07           = %d007 / %d135 x00

L             = %d204
o             = %d239
g             = %d231
i             = %d233
w             = %d247
e             = %d229
b             = %d226
id-version    = %d001
id-Logiweb    = L o g i w e b id-version

timestamp     = mantissa exponent
mantissa      = cardinal
exponent      = cardinal

message       = ping
ping          = id-ping
id-ping       = x02

message       =/ pong
pong          = id-pong id-Logiweb timestamp
id-pong       = x03

message       =/ event
event         = id-event notice
id-event      = x01
notice        = sorry / received / rejected
sorry         = x00
received      = x01
rejected      = x02

message       =/ nop
nop           = id-nop
id-nop        = x00
message       =/ prefix
prefix        = id-prefix code contents
id-prefix     = x07
code          = cardinal
contents      = message

vector        = length bytes
length        = cardinal
bytes         = *byte
byte          = %d0-255
message       =/ get
get           = id-get address class index
id-get        = x04
address       = vector
class         = update / type / left / right
class         =/ sibling / url / leap
update        = x00
type          = x01
left          = x02
right         = x03
sibling       = x04
url           = x05
leap          = x06
index         = cardinal
leap          = step mjd
step          = cardinal
mjd           = cardinal

message       =/ got
got           = id-got address class index
                         norm count timestamp value
id-got        = x05
norm          = cardinal
count         = cardinal
value         = vector

message       =/ put
put           = id-put address class operation value
id-put        = x06
operation     = remove / add
remove        = x00
add           = x01

document      = id-version ripemd timestamp contents
id-version    = %d001
ripemd        = 20*20 byte
contents      = *byte
byte          = %d0-255

