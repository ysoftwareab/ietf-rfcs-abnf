id       = basicid / extid

basicid  = notlb *idchar
notlb    = %x00-7a / %x7c-7f ; anything other than a "{"

extid    = "{" extname "}" extarg
extname  = ALPHA *( ALPHA / DIGIT / "+" / "-" / "." )
extarg   = *idchar
idchar   = %x00-ff ; any encodable character
ALPHA    = %x41-5a / %x61-7a
DIGIT    = %x30-39

xmlns="urn:oasis:names:tc:SAML:2.0:metadata">
