Billing-Info   = "Billing-Info" ":" hostport "<" Acct-Data ">"

Acct-Data      = 1*unreserved | (1*unreserved "," Acct-Data)

Billing-ID        = "Billing-ID" ":" 1*unreserved


Gate              = "Gate" ":" hostport "/" Gate-ID
                                   [";" Gate-Key ";" Gate-CipherSuite]


Gate-ID           = 1*alphanum

Gate-Key          = 1*alphanum

Gate-CipherSuite  = token


