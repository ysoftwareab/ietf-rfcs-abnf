ALPHA = %x41-5A / %61-7A     ; A-Z / a-z

LDIGIT = %x31-39             ; 1-9

DIGIT = %x30 / ld            ; 0-9

HYPHEN = %x2D                ; "-"

PERIOD = %x2E                ; "."

number = DIGIT / ( LDIGIT 1*DIGIT )

keychar = ( ALPHA / DIGIT / HYPHEN )
leadkeychar = ALPHA

keystring = leadkeychar *keychar

name = keystring

numericoid = number *( PERIOD number ) ; e.g. 1.1.0.23.40

option = keystring

M => Matching Rule
O => Object Class
U => LDAP URL Extension
T => Attribute Type


