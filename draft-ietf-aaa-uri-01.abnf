aaaURI             = "aaa:" hostport [ transport ]
                                                [ protocol ]
aaasURI            = "aaas:" hostport [ transport ]
                                                 [ protocol ]
transport          = ";" "transport=" transport-protocol
transport-protocol = ( "tcp" / "sctp" / "udp" )
protocol           = ";" "protocol=" aaa-protocol
aaa-protocol       = ( "diameter" / "radius" / "tacacs+" )

