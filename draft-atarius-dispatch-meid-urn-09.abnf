pp2-urn         = "urn:" pp2-NID ":" pp2-NSS
pp2-NID         = "3gpp2"
pp2-NSS         = meid-specifier
meid-specifier  = "meid:" meidval


meidval           = Manufacturer-Code "-" Serial-Number
Manufacturer-Code = 8HEX
Serial-Number     = 6HEX
HEX               = DIGIT / "A" / "B" / "C" / "D" / "E" / "F"

