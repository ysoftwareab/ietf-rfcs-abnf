arcp-URI       = arcp-scheme ":" arcp-specific [ "#" fragment ]

arcp-scheme    = "arcp"

arcp-specific  = "//" arcp-authority [ path-absolute ] [ "?" query ]

arcp-authority = uuid | ni | name | authority
uuid          = "uuid," UUID
ni            = "ni," alg-val
name          = "name," reg-name

alt="App" />
