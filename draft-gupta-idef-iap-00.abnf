iap-message       =
                          ( iap-connect-request |
                            iap-upgrade-request |
                            iap-version-verify  |
                            iap-content         |
                            iap-response )
                           CRLF

iap-t-version     = "IAP/0.1"
iap-response      = iap-t-version SP
                         3DIGIT CRLF

iap-t-via         = iap-t-version SP
                         "VIA" SP host CRLF

iap-content-type  = "Content-Type:" SP
                         "application/x-idef-alert"
                         CRLF

