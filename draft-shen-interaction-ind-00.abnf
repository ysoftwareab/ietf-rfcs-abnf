PIndicator =  
      "P-Interaction-Indicator" HCOLON indicatorEntry  
      *(COMMA Indicator-entry)  
indicatorEntry = "<" as-uri (SEMI indicationDirection) 
as-uri= name-addr  
indicationDirection = "forward"/ "backward" 
indicatorParam = indToken *(HCOLON application)  
indToken = "allow" / "reject" 
application = application-string *(COMMA application-string) 
application-string = "conferenceRequest"/ 
application-extension = token 
       
    
