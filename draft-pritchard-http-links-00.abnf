Method        = "LINK"
                           | "UNLINK"
                           | "UNLINKR"
                           | "LINKMOD"

Request       = Link-Request-Line
                           | Unlink-Request-Line
                           | UnlinkR-Request-Line
                           | LinkMod-Request-Line
                           *( general-header  )      ; HTTP/1.1 07 4.5
                           CRLF

Response      = Status-Line  ; As HTTP/1.1

Status-Code   = "200"   ; Ok
                           | "207"   ; No Linkmod
                           | "400"   ; Bad Request
                           | "404"   ; Not found
                           | "416"   ; Invalid source URI
                           | "417"   ; Invalid target URI
                           | "500"   ; Internal Server Error

