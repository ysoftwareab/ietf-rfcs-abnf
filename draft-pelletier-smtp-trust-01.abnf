ORIGIN=identity@domain-spec

UPALPHA          = %x41-5A

LOALPHA          = %x61-7A

ALPHA            = UPALPHA / LOALPHA

DIGIT            = %x30-39

PRINTABLE        = %x21-7E

domain-char      = DIGIT / ALPHA / %x2D

sub-domain       = 1#(domain-char)

domain-spec      = sub-domain *("." sub-domain)

identity-char    = DIGIT / ALPHA / %x21-%2f

identity         = 1#(identity-char)

challenge-string = 1#(PRINTABLE)

toro-command     = "TORO" SPACE domain-spec SPACE challenge-string
origin-param     = "ORIGIN=" identity "@" domain-spec
