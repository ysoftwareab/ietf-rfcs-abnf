ssh_URI = "ssh://" [ userinfo ] [ ";"conn-parameter=value ] [ "@" ]
        host [ ":" port ]

scp_URI = "scp://" [userinfo ] [ ";"conn-parameter=value ] [ "@" ]
        host [ ":" port ] [abs_path ]

sftp_URI = "sftp://" [ userinfo ] [ ";"conn-parameter=value] [ "@" ]
        host [ ":" port ] [ abs_path ] [ ";"sftp-parameter=value ]

