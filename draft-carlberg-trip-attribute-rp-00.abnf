Resource-Priority  = "Resource-Priority" HCOLON
                              r-value *(COMMA r-value)
r-value            = namespace "." r-priority
namespace          = token-nodot
r-priority         = token-nodot
token-nodot        = 1*( alphanum / "-"  / "!" / "%" / "*"
                                     / "_" / "+" / "`" / "'" / "~" )

namespace = 1*( alphanum / "-" / "!" / "%" / "*" / "_" / "+"
                   / "`" / "'" / "~" )


