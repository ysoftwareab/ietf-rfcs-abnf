rights_capa      = "RIGHTS=" new_rights
                      ;; RIGHTS=... capability

new_rights       = rights
                      ;; MUST include "t", "e", "x", "m" and "n"

resp-text-code   =/ myrights_rspcod

myrights_rspcod  = "MYRIGHTS" SP rights

rights           = atom
                      ;; MUST NOT contain leading "+" or "-"
                      ;; MUST be treated as case-sensitive


