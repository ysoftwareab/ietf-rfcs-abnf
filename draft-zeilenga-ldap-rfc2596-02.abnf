language-tag-option = "lang-" Language-Tag

Language-Tag = Primary-subtag *( "-" Subtag )

Primary-subtag = 1*8ALPHA

Subtag = 1*8(ALPHA / DIGIT)

ALPHA = %x41-5A / %x61-7A   ; A-Z / a-z

DIGIT = %x30-39             ; 0-9

language-range-option = "lang-" [ Language-Tag "-" ]

Language-Tag = Primary-subtag *( "-" Subtag )

Primary-subtag = 1*8ALPHA

Subtag = 1*8(ALPHA / DIGIT)

ALPHA = %x41-5A / %x61-7A   ; A-Z / a-z

DIGIT = %x30-39             ; 0-9

