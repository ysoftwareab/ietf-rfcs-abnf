esl       =  scheme ":" hash ":" sig ":" label

scheme    =  "esl"
hash      =  1*alphanum                  ; from [RFC-2396]
sig       =  1*sigchar
label     =  8*labelchar
sigchar   =  unreserved / escaped        ; from [RFC-2396]
labelchar =  alphanum / "-"              ; from [RFC-2396]

eslreg = r'esl:([^:]+):([^:]+):(.*)'
esl = Esl(form['esl'].value)
form = cgi.FieldStorage()
form_ok = 0
esl = Esl(str(sys.argv[1]))
