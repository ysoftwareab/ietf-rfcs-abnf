fields     =/ downgraded
downgraded =  "Downgraded:" [FWS] field-name ":" unstructured CRLF


charset='UTF-8'.
fields      =/ edowngraded
edowngraded = "Envelope-Downgraded:" [FWS] edowngraded-field ":"
                                        [FWS] "<" uPath ">" [FWS]
                                        "<" Mailbox ">" [FWS] CRLF
edowngraded-field =  "From" / "To"

