ippget_URL = "ippget:" "//" authority [ abs_path [ "?" query ]]
authority  = server | reg_name
reg_name   = 1*( unreserved | escaped | "$" | "," |
                       ";" | ":" | "@" | "&" | "=" | "+" )
server     = [ [ userinfo "@" ] hostport ]
userinfo   = *( unreserved | escaped |
                      ";" | ":" | "&" | "=" | "+" | "$" | "," )
hostport   = host [ ":" port ]
abs_path   = "/"  path_segments

