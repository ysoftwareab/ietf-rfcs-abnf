delim    = %x01

command  = 1*( %x02-09 / %x0B-0C / %x0E-1F / %x21-FF )
                      ; any octet except NUL, delim, CR, LF, and " "

params   = 1*( %x02-09 / %x0B-0C / %x0E-FF )
                      ; any octet except NUL, delim, CR, and LF

body     = delim command [ SPACE params ] [ delim ]

