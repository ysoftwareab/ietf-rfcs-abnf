start = acme-record / bundle / tstr

acme-record = {
     id-chal,
     token-bundle,
     ? key-auth-digest ; present in Response Bundles
     ? alg-list ; present in Challenge Bundles
id-chal = (1 => bstr)
token-bundle = (2 => bstr)
key-auth-digest = (3 => [
       alg: alg-id,
       value: bstr
alg-list = (4 => [+ alg-id])
