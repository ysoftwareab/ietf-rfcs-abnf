cidurl     = "cid" ":" id-spec

midurl     = msgmid / cidmid

msgmid     = "mid" ":" id-spec

cidmid     = msgmid "#" cidurl

id-spec    = local-part "@" hostname
                                           ; globally unique

local-part = 1*uchar

content-id = "<" id-spec ">".

message-id = "<" id-spec ">".

