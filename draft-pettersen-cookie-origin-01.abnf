attr        =     token
value       =     token | quoted-string

cookie          =  "Cookie:" cookie-value 0*(";" cookie-value)
cookie-value    =  NAME "=" VALUE ";" cookie-origin

NAME            =  attr
VALUE           =  value
cookie-origin   =  "$Origin" "=" <"> http_URL <">


