Outer_Wrapper = {
msg-authenc-wrapper         => bstr .cbor
                                      Msg_AuthEnc_Wrapper / nil,
teep-message                => (QueryRequest /
                                       QueryResponse /
                                       TrustedAppInstall /
                                       TrustedAppDelete /
                                       Error /
                                       Success ),
msg-authenc-wrapper = 1
teep-message = 2

Msg_AuthEnc_Wrapper = [ * (COSE_Mac_Tagged /
                              COSE_Sign_Tagged /
                              COSE_Mac0_Tagged /
                              COSE_Sign1_Tagged)]

suite = int

version = int

data_item = int

QueryRequest = {
        TYPE : int,
        TOKEN : bstr,
        REQUEST : [+data_item],
        ? CIPHER_SUITE : [+suite],
        ? NONCE : bstr,
        ? VERSION : [+version],
        ? OCSP_DATA : bstr,
        * $$extensions
ext_info = int

QueryResponse = {
        TYPE : int,
        TOKEN : bstr,
        ? SELECTED_CIPHER_SUITE : suite,
        ? SELECTED_VERSION : version,
        ? EAT : bstr,
        ? TA_LIST  : [+bstr],
        ? EXT_LIST : [+ext_info],
        * $$extensions
TrustedAppInstall = {
        TYPE : int,
        TOKEN : bstr,
        ? MANIFEST_LIST  : [+ SUIT_Outer_Wrapper],
        * $$extensions
TrustedAppDelete  = {
        TYPE : int,
        TOKEN : bstr,
        ? TA_LIST  : [+bstr],
        * $$extensions
Success = {
        TYPE : int,
        TOKEN : bstr,
        ? MSG : tstr,
        * $$extensions
Error = {
        TYPE : int,
        TOKEN : bstr,
        ERR_CODE : int,
        ? ERR_MSG : tstr,
        ? CIPHER_SUITE : [+suite],
        ? VERSION : [+version],
        * $$extensions
