xmppuri   = "xmpp:" [ nodeid "@" ] host [ path-absolute ]
                  [ "?" xmppquery ] [ "#" fragment ]
nodeid    = *( unreserved / pct-encoded / allowed )
allowed   = "!" / "$" / "(" / ")" / "*" / "+" / ";" / "="
xmppquery = querytype [ *pair ]
querytype = *( ALPHA / DIGIT / '-' / '_' / '.' / ':' )
pair      = "&" key "=" value
key       = *unreserved
value     = *( unreserved / pct-encoded )


xmppuri = "xmpp:" [ nodeid "@" ] host [ path-absolute ]
                [ "?" query ]
nodeid  = *( unreserved / pct-encoded / allowed )
allowed = "!" / "$" / "(" / ")" / "*" / "+" / ";" / "="



