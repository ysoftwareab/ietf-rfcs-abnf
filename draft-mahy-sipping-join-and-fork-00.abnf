cc-header = ( join | fork | token ) ":" SP call-info

join      = "Join"
fork      = "Fork"

call-info = callid *( ";" header-params )

header-params = ( to-tag | from-tag | extension )

callid    =  token ["@" token]
to-tag    = "to-tag="   UUID
from-tag  = "from-tag=" UUID
extension =  param [ "=" val ]
param     =  token
val       =  token | quoted-string


