isupport     =  [ ":" servername SP ] "005" SP nick SP
   	                  1*13( token SP ) ":are supported by this server"

token        = *1"-" parameter / parameter *1( "=" value )
parameter    = 1*20letter
value        = *letpun
letter       = ALPHA / DIGIT
punct        = %d33-47 / %d58-64 / %d91-96 / %d123-126
letpun       = letter / punct

CASEMAPPING=string

CASEMAPPING=rfc1459

CHANLIMIT=prefix:number[,prefix:number[,...]]

CHANLIMIT=#+:25,&:
CHANMODES=A,B,C,D

CHANMODES=b,k,l,imnpst

CHANNELLEN=number

CHANNELLEN=50
CHANTYPES=[string]

CHANTYPES=&#
ELIST=string

ELIST=CMNTU

MAXLIST=mode:number[,mode:number[,...]]

MAXLIST=beI:25
MAXLIST=b:25,eI:50
MODES=[number]

MODES=3
NETWORK=string

NETWORK=EFnet
NICKLEN=number

NICKLEN=9
PREFIX=[(modes)prefixes]

PREFIX=(ov)@+
SILENCE=number

SILENCE=15
STATUSMSG=string

STATUSMSG=@+
TARGMAX=[cmd:number,cmd:number,...]

TARGMAX=PRIVMSG:3,WHOIS:1,JOIN:
TOPICLEN=number

TOPICLEN=120
WATCH=number

WATCH=100
