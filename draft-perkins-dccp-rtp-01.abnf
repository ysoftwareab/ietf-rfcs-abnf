media-field = "m=" media SP port ["/" integer] SP proto
                     1*(SP fmt) CRLF

dccp-service-attr = "a=dccp-service-code:" service-code

service-code      = hex-sc / decimal-sc / ascii-sc

hex-sc            = "SC=x" *HEXDIG

decimal-sc        = "SC="  *DIGIT

ascii-sc          = "SC:"  *sc-char

sc-char           = %d42-43, %d45-47, %d63-90, %d95, %d97-122


