dnsurl       = "dns:" [ "//" hostport "/" ] hostname
                  ";TYPE=" type [";TYPE=" class]
                                ; See RFC 2396 for "hostport" and
                                ; "hostname" definition

type         = "A" / "NS" / "MD" / ...
                                ; Any standard DNS type

class        = "IN" / "CH" / ...
                                ; Any standard DNS class

