op          = 1*ALPHA
arg         = *(reserved / unreserved / pct-encoded)
var         = varname [ "=" vardefault ]
vars        = var [ *("," var) ]
varname     = (ALPHA / DIGIT)*(ALPHA / DIGIT / "." / "_" / "-" )
vardefault  = *(unreserved / pct-encoded)
operator    = "-" op "|" arg "|" vars
expansion   = "{" ( var / operator ) "}"


op = op[1:]
vars_ = vars_.split(",")

variables = {}
