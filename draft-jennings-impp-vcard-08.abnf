param      = impp-param ; Only impp parameters are allowed

value      = URI
                 ; URI defined in Section 3 of [3]

impp-param = "TYPE" "=" impp-type *("," impp-type)

impp-type  = "PERSONAL" / "BUSINESS" / ; purpose of communications
                 "HOME" / "WORK" / "MOBILE" /
                 "PREF" /
                 iana-token / x-name;
                 ; Values are case insensitive


