ALPHA           =  %x41-5A / %x61-7A   ; A-Z / a-z
DIGIT           =  %x30-39             ; 0-9
HEXDIG          =  DIGIT / "A" / "B" / "C" / "D" / "E" / "F"

Marlin-NSS      =   1*(subStChar) 0*(":" 1*(subStChar))
subStChar       =   trans / "%" HEXDIG HEXDIG
trans           =   ALPHA / DIGIT / other / reserved
other           =   "(" / ")" / "+" / "," / "-" / "." /
                          "=" / "@" / ";" / "$" /
                          "_" / "!" / "*" / "'"
reserved        =   "%" / "/" / "?" / "#"

