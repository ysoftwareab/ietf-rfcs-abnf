av-pairs        =       av-pair *(";" av-pair) 
av-pair         =       attr ["=" value]        ; optional value 
attr            =       token 
value           =       word 
word            =       token | quoted-string 
cookie          =       "Cookie:" cookies 
cookies         =       1#cookie 
cookie          =       NAME "=" VALUE *(";" cookie-av) 
NAME            =       attr 
VALUE           =       value 
cookie-av       =       "Comment" "=" value 
                      |       "Domain" "=" value 
                      |       "Version" "=" 1*DIGIT 
    
NAME=VALUE 
    
Comment=comment 
    
Domain=domain 
    
Version=version 
    
