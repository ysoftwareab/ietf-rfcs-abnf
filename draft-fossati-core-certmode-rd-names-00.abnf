RD-char = ALPHA / DIGIT / "-" / "_" / "~" / "!" /
               "$" / "&" / "'" / "(" / ")" / "*" /
               "," / ";" / "="
RD-ns = ALPHA *(ALPHA / DIGIT / "-")  ; the name-space
RD-name = 1*RD-char
RD-domain = 1*63RD-char
RD-authority = [ RD-ns "+" ] RD-name [ "." RD-domain ] [ ":" port ]

