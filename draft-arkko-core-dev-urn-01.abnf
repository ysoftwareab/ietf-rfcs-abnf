devurn = "urn:dev:" subtype ":" hexstring componentpart
subtype = "mac" / "ow" / "cgi"
componentpart = [ "-" component [ componentpart ]]
component = *1(DIGIT / ALPHA)
hexstring = hexbyte /
                 hexbyte hexstring
hexbyte = hexdigit hexdigit
hexdigit = DIGIT / hexletter
hexletter = "a" / "b" / "c" / "d" / "e" / "f" /
                 "A" / "B" / "C" / "D" / "E" / "F"

