Event       =  "Event" ":" event-type
                    *(( ";" parameter-name ["=" token] )
                    ( ";" parameter-name ["=" quoted-string] ))
event-type  =  token


Allow-Events =  "Allow-Events" ":" 1#event-type


Client-Error = "400"  ; Bad Request
                  ...
                  | "489"  ; Bad Event


