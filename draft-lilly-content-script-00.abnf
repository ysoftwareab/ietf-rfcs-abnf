content-script = "Content-Script" ":" [CFWS] script-list [CFWS] CRLF

script-list = script *([CFWS] "," [CFWS] script)

script = 4*ALPHA ; script tag per [N4.ISO15924]; [I2.15924Lists]
                    ; script tags are case-insensitive protocol elements
                    ; [I3.RFC1958]

accept-script = "Accept-Script" ":" [CFWS] script-list CRLF

script-q-list = script-q [CFWS] 0*("," [CFWS] script-q [CFWS])

script-q = (script / "*") [CFWS]
              [";" [CFWS] "q" [CFWS] "=" [CFWS] qvalue]

qvalue = ( "0" [ "." 0*3DIGIT ] )
          / ( "1" [ "." 0*3("0") ] )

