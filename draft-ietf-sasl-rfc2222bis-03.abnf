sasl-mech    = 1*20mech-char
mech-char    = %x41-5A / DIGIT / "-" / "_"
                  ; mech names restricted to uppercase ASCII letters,
                  ; digits, "-" and "_"


extern-init-resp  = *( UTF8-char-no-nul )

UTF8-char-no-nul  = UTF8-1-no-nul / UTF8-2 / UTF8-3 / UTF8-4

UTF8-1-no-nul     = %x01-7F


