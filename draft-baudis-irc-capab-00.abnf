message   =  "HANDSHAKE" CRLF

message   =  "REGISTER" <CRLF>

message =  "CAP" 1*SP type [ 1*SP ":" token ] CRLF
type    =  "LS" / "ENDLS" / "RQ" / "ACK" / "NAK"
token   =  [ "-" ] name [ "=" value ] [ 1*SP token ]
name    =  letterS *19letter
value   =  1*letter

letterS =  ALPHA / DIGIT / "_"
letter  =  ALPHA / DIGIT / "_" / "-"

