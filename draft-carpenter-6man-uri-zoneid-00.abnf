IP-literal = "[" ( IPv6address / IPvFuture  ) "]"

ZoneID = 1*15unreserved

IPv6address =  6( h16 ":" ) ls32
                   / "::" 5( h16 ":" ) ls32
                   / [               h16 ] "::" 4( h16 ":" ) ls32
                   / [ *1( h16 ":" ) h16 ] "::" 3( h16 ":" ) ls32
                   / [ *2( h16 ":" ) h16 ] "::" 2( h16 ":" ) ls32
                   / [ *3( h16 ":" ) h16 ] "::"    h16 ":"   ls32
                   / [ *4( h16 ":" ) h16 ] "::"              ls32
                   / [ *5( h16 ":" ) h16 ] "::"              h16
                   / [ *6( h16 ":" ) h16 ] "::"
                   / "fe80::" [ *3( h16 ":" ) h16 ] "%" ZoneID

IP-literal = "[" ( IPv6addrz / IPvFuture  ) "]"

IPv6addrz = IPv6address [ "%" ZoneID ]

