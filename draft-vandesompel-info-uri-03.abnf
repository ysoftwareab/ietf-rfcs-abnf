info-URI        = info-scheme ":" info-identifier [ "#" fragment ]

info-scheme     = "info"

info-identifier = namespace "/" identifier

namespace       = scheme

identifier      = *( pchar / "/" )

scheme          = ALPHA *( ALPHA / DIGIT / "+" / "-" / "." )

pchar           = unreserved / pct-encoded / sub-delims / ":" / "@"

fragment        = *( pchar / "/" / "?" )

unreserved      = ALPHA / DIGIT / "-" / "." / "_" / "~"

pct-encoded     = "%" HEXDIG HEXDIG

sub-delims      = "!" / "$" / "&" / "'" / "(" / ")"
                     / "*" / "+" / "," / ";" / "="

