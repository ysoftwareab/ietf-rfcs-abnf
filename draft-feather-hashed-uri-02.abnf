hashed-uri = hashed-scheme ":" hash-data *flag

hash-data = hash-algorithm "=" hash-value

hashed-scheme = "hashed"

hash-algorithm = identifier

hash-value = 1*HEXDIG

identifier = 1*(ALPHA / DIGIT / "." / "-")

flag = "+frag" / "+query"

ALPHA  = %x41-5A / %x61-7A   ; A-Z / a-z

DIGIT  = %x30-39             ; 0-9

HEXDIG =  DIGIT / "A" / "B" / "C" / "D" / "E" / "F"

