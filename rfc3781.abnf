membersKeyword      =  %x6D %x65 %x6D %x62 %x65 %x72 %x73
mandatoryKeyword    =  %x6D %x61 %x6E %x64 %x61 %x74 %x6F %x72 %x79
optionalKeyword     =  %x6F %x70 %x74 %x69 %x6F %x6E %x61 %x6C
