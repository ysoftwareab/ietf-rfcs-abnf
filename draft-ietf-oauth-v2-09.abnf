challenge       = "Token" RWS token-challenge

token-challenge = realm
                       [ CS error ]
                       [ CS error-desc ]
                       [ CS error-uri ]
                       [ CS scope ]
                       [ CS 1#auth-param ]

error           = "error" "=" <"> token <">
error-desc      = "error-description" "=" quoted-string
error-uri       = "error-uri" = <"> URI-Reference <">
scope           = ptoken / <"> ptoken *( 1*SP ptoken ) <">

ptoken          = 1*ptokenchar
ptokenchar      =   "!" / "#" / "$" / "%" / "&" / "'" / "("
                       / ")" / "*" / "+" / "-" / "." / "/" / DIGIT
                       / ":" / "<" / "=" / ">" / "?" / "@" / ALPHA
                       / "[" / "]" / "^" / "_" / "`" / "{" / "|"
                       / "}" / "~"


param-name  = 1*fchar
fchar       = "-" / "." / "_" / DIGIT / ALPHA
param-value  = ptoken | quoted-string


