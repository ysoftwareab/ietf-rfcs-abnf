KDF = PRF(key, US_data)

T1 = PRF (Key, US_Data | 0x01)

T2 = PRF (Key, T1 | US_Data | 0x02)

T3 = PRF (Key, T2 | US_Data | 0x03)

T4 = PRF (Key, T3 | US_Data | 0x04)

T1 = HMAC-SHA-256 (EMSK, US_Data | 0x01)

T2 = HMAC-SHA-256 (EMSK, T1 | US_Data | 0x02)

T3 = HMAC-SHA-256 (EMSK, T2 | US_Data | 0x03)

T4 = HMAC-SHA-256 (EMSK, T3 | US_Data | 0x04)

US_data=Key label + "\0" + op-data + length
op-data = optional data

length = 2 byte unsigned integer in network byte order

MIP6_T1 = HMAC-SHA-256 (EMSK, "MIP6 USRK derivation"|<MSA_ID>|
MIP6_T2 = HMAC-SHA-256 (EMSK, MIP6_T1 | "MIP6 USRK derivation" |
MIP6_USRK=MIP6_T1 | MIP6_T2

MIP6_MN_HA_key = HMAC-SHA-256(MIP6_USRK, "MN-HA key derivation"|
MIP6_IKEv2_key=HMAC-SHA-256(MIP6_USRK, "IKEv2 pre-shared key
MIP4_T1 = HMAC-SHA-256 (EMSK, "MIP4 USRK derivation"|AAAH_ID |MN_ID|
MIP4_T2 = HMAC-SHA-256 (EMSK, MIP4_T1 | "MIP4 USRK derivation" |
MIP4_USRK=MIP4_T1 | MIP4_T2

MN_HA_MIP4_key = HMAC-SHA-256(MIP4_USRK, "MN-HA key derivation"|
PMN_HA_MIP4_key = HMAC-SHA-256(MIP4_USRK, "PMN-HA key derivation"|
MN_FA_MIP4_key = HMAC-SHA-256(MIP4_USRK , "MN-HA MIP4 key
FA_HA_MIP4_Root = HMAC-SHA-256(MIP4_USRK, "FA-MIP4 root key
