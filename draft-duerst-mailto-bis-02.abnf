mailtoURI   = "mailto:" [ to ] [ headers ]
to          = [ addr-spec *("%2C" addr-spec ) ]
headers     = "?" header *( "&" header )
header      = hname "=" hvalue
hname       = *qchar
hvalue      = *qchar
addr-spec   = local-part "@" domain
local-part  = dot-atom / quoted-string
qchar       = unreserved / pct-encoded / some-delims
some-delims = "!" / "$" / "'" / "(" / ")" / "*"
                   / "+" / "," / ";" / ":" / "@"

