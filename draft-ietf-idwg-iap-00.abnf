iap-message       =
                          ( iap-connect-request |
                            iap-upgrade-request |
                            iap-verify          |
                            iap-content         |
                            iap-response )
                           CRLF




iap-t-version     = "IAP/0.2"
iap-t-sender     = "IAP-Role: Sender" CRLF





iap-response      = iap-t-version SP
                         3DIGIT CRLF





iap-t-via         = iap-t-version SP
                         "VIA" SP host CRLF
iap-content-type  = "Content-Type:" SP
                         "application/x-idef-alert"
                         CRLF

