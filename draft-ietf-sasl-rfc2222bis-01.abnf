sasl-mech    = 1*20mech-char
mech-char    = %x41-5A / DIGIT / "-" / "_"
                  ; mech names restricted to uppercase letters,
                  ; digits, "-" and "_"
initial-response  = *( UTF8-char-no-null )

UTF8-char-no-null = UTF8-1-no-null / UTF8-2 / UTF8-3 / UTF8-4

UTF8-1-no-null    = %x01-7F


