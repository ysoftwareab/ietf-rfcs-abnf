Supersedes-field          = "Supersedes:" CFWS identifier
                                *(identifier)
                                optional-parameter-list
                                [CFWS] CRLF

optional-parameter-list   = *( ";" LWSP parameter )

parameter                 = parameter-name [ "="
                                parameter-value ]

parameter-name            = "noshow" / "show" / "repost"
                                private-parameter /
                                future-parameter

