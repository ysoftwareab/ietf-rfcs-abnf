bit-string-label =  "\[" bit-spec "]"

bit-spec         =  bit-data [ "/" length ]
                        / dotted-quad [ "/" slength ]

bit-data         =  "x" 1*64HEXDIG
                        / "o" 1*86OCTDIG
                        / "b" 1*256BIT

dotted-quad      =  decbyte "." decbyte "." decbyte "." decbyte

decbyte          =  1*3DIGIT

length           =  NZDIGIT *2DIGIT

slength          =  NZDIGIT [ DIGIT ]

OCTDIG           =  %x30-37

NZDIGIT          =  %x31-39

