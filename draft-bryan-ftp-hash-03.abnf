hash = "HASH" SP <pathname>

hash-response = "213" SP 1*HEXDIGIT CRLF

hash-feat = SP "HASH" SP hashlist CRLF
hashlist = 1*( hashname ["*"] ";" )
hashname = 1*( hchar )
hchar = ALPHA / DIGIT / "-" / "_" / "/" / "." / ","


hashopts-cmd = "OPTS HASH" [ SP hashtype ] CRLF
hashopts-response = "200" SP hashtype CRLF


