linked-uri = scheme ":" [options] "@" <absolute-URI>
scheme = "pgpid+cookie"
options = ( option / flag ) [";" options]
option = key "=" value
flag = key
key = *(<unreserved>)
value = *(<unreserved> / <pct-encoded> / ",")

cookie = "[" "Verifying my OpenPGP key " cookieuri  "]"
cookieuri = "openpgp4fpr" ":" fingerprint
fingerprint = 40<HEXDIG>

