geo-URI       = geo-scheme ":" geo-path
geo-scheme    = "geo"
geo-path      = geo-location

geo-location   = latitude "," longitude [ "," altitude ]

latitude       = [ "-" ] 1*2DIGIT [ "." *DIGIT ]
longitude      = [ "-" ] 1*3DIGIT [ "." *DIGIT ]
altitude       = [ "-" ] *DIGIT [ "." *DIGIT ]


